{
  "name": "web-app",
  "version": "1.0.0",
  "title": "Basic Web-Backend Application",
  "description": "Basic Web Backend application",
  "template": "template/template.json",
  "layout": "main-layout",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "modules"
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "environment": "development",
        "backend": "backend",
        "default": true
      },
      {
        "name": "backend",
        "platform": "backend",
        "environment": "development"
      }
    ]
  }
}
